      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "data = [['Berti',90,85,95,90.5],\n",
        "       ['Qorygore',80,85,90,86.6],\n",
        "       ['Bimo',70,75,80,78.5]]\n",
        "index = [0,1,2]\n",
        "kolom = ['Nama','Tugas','UTS','UAS','Rata-Rata']\n",
        "df = pd.DataFrame(data,index,kolom)\n",
        "df"
